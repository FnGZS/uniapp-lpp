<view><view id="cleanNoraml"><view class="cleanNoraml-search"><view class="cleanNoraml-search-box"><view class="cuIcon-search font_30"></view><input type="text" placeholder="请输入名字搜索" data-event-opts="{{[['change',[['input_searchName',['$event']]]]]}}" value="{{searchName}}" bindchange="__e"/></view></view><view id="swiper-tab"><view class="swiper-tab"><view class="{{['swiper-tab-list',currentTab==0?'on':'']}}" data-current="0" data-event-opts="{{[['tap',[['tabNav',['$event']]]]]}}" bindtap="__e">{{first+''}}<view class="_i"></view></view><view class="{{['swiper-tab-list',currentTab==1?'on':'']}}" data-current="1" data-event-opts="{{[['tap',[['tabNav',['$event']]]]]}}" bindtap="__e">{{second+''}}<view class="_i"></view></view><view class="{{['swiper-tab-list',currentTab==2?'on':'']}}" data-current="2" data-event-opts="{{[['tap',[['tabNav',['$event']]]]]}}" bindtap="__e">{{thirds+''}}<view class="_i"></view></view></view><swiper class="swiper-item" style="{{'display:'+(displays)+';'}}" current="{{currentTab}}" duration="500"><swiper-item><view class="location_bottom"><block wx:for="{{cityArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[city_index==index?'add_citying':'add_city']}}" data-num="{{index}}" data-name="{{item.value}}" data-event-opts="{{[['tap',[['clickCity',['$event']]]]]}}" bindtap="__e">{{item.name}}</view></block></view></swiper-item><swiper-item><view class="location_bottom"><block wx:for="{{priceArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[price_index==index?'add_priceing':'add_price']}}" data-num="{{index}}" data-name="{{item.value}}" data-event-opts="{{[['tap',[['clickPrice',['$event']]]]]}}" bindtap="__e">{{item.value}}</view></block></view></swiper-item><swiper-item><view class="location_bottom"><block wx:for="{{amountArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[amount_index==index?'add_amounting':'add_amount']}}" data-num="{{index}}" data-name="{{item.value}}" data-event-opts="{{[['tap',[['clickAmount',['$event']]]]]}}" bindtap="__e">{{item.value}}</view></block></view></swiper-item></swiper></view><view data-event-opts="{{[['tap',[['hideNav',['$event']]]]]}}" class="swiper-beijing" style="{{'display:'+(displays)+';'}}" bindtap="__e"></view><view class="listMain"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list"><view class="list-left"><image class="list-left-img" src="https://www.sxscott.com/img/c.png"></image><view class="list-left-line"></view><view class="list-left-phone"><view class="iconfont icon-dianhua" data-phone="{{item.telephone}}" data-event-opts="{{[['tap',[['phone',['$event']]]]]}}" bindtap="__e"></view></view></view><view class="list-center"><view class="list-name">{{item.name}}</view><view class="list-time">{{item.work+"年保洁工龄"}}</view><view class="list-num"><view class="list-num-jiedan"><view class="list-num-jiedan-text">接单量</view><view class="list-num-jiedan-number">{{item.total}}</view></view><view class="list-num-tuidan"><view class="list-num-tuidan-text">退单量</view><view class="list-num-tuidan-number">{{item.back}}</view></view></view><view class="list-card"><view class="label cu-tag  bg-orange">{{item.card}}</view></view><view class="list-place">{{''+item.place+''}}</view></view><view class="list-right"><view class="iconfont icon-biaoqian"><view class="list-score">{{item.score}}</view></view><view class="list-yuyue" data-id="{{item.id}}" data-event-opts="{{[['tap',[['toDetail',['$event']]]]]}}" bindtap="__e">预约</view></view></view></block></view></view></view>