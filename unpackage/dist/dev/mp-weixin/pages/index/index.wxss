
page {
	background-color: #fff !important;
}
/* 登录 */
.cu-login {
	height: 80rpx;
	font-size: 35rpx;
}
/* 轮播 */
.swiper_container {
	height: 55rpx;
	width: 100vw;
	margin-left: 10rpx;
}
.swiper_item {
	font-size: 26rpx;
	line-height: 55rpx;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
	letter-spacing: 2px;
	color: #b1b1b1;
}
.screen-swiper {
	width: 750rpx;
	min-height: 320rpx;
}
/* 公告部分 */
.gonggao {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	width: 750rpx;
	height: 74rpx;
	border-bottom: 8rpx solid #f8f8f8;
	background: #fff;
}
.gonggao-left {
	position: relative;
	width: 184rpx;
	height: 34rpx;
	border-right: 2rpx solid #c3c3c3;
}
.gonggao-box {
	position: absolute;
	z-index: 2;
	right: 30rpx;
	bottom: -2rpx;
	width: 78rpx;
	height: 36rpx;
	background: -webkit-gradient(linear, left top, right top, from(#ff8a00), to(#ffce08));
	background: -o-linear-gradient(left, #ff8a00, #ffce08);
	background: linear-gradient(to right, #ff8a00, #ffce08);
	color: #fff;
	text-align: center;
	border-radius: 4rpx;
}
.gonggao-rightArrow {
	position: absolute;
	bottom: 4rpx;
	right: 16rpx;
	z-index: 1;
	width: 0;
	height: 0;
	border-top: 0 solid transparent;
	border-bottom: 14rpx solid transparent;
	border-left: 16rpx solid #ff8a00;
	opacity: 0.42;
}
/* 常规保洁 */
.baojie-cont {
	padding: 0 20rpx 30rpx 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	border-bottom: 8rpx solid #f8f8f8;
}
.baojie-list {
	margin: 0 10rpx 10rpx 0;
	width: 340rpx;
	height: 180rpx;
	background: #f5f5f5;
	position: relative;
}
.baojie-list-title {
	color: #333333;
	margin-top: 24rpx;
	margin-left: 20rpx;
}
.baojie-list-text {
	color: #777777;
	margin-top: 10rpx;
	margin-left: 20rpx;
}
.baojie-list-go {
	color: #b19c71;
	border: 1rpx solid #b19c71;
	text-align: center;
	border-radius: 4rpx;
	width: 60rpx;
	padding: 2rpx 5rpx;
	margin-left: 20rpx;
	margin-top: 10rpx;
}
.baojie-list image {
	position: absolute;
	right: 20rpx;
	top: 20rpx;
	width: 100rpx;
	height: 140rpx;
}
/* 推荐 */
.tuijian-cont {
	padding: 0 30rpx;
	margin-bottom: 50rpx;
}
.tuijian-list {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	position: relative;
	-webkit-box-shadow: 0px 3px 5px 1px rgba(0, 0, 0, 0.1);
	        box-shadow: 0px 3px 5px 1px rgba(0, 0, 0, 0.1);
	border: 1rpx solid #f4f4f4;
	margin-bottom: 20rpx;
}
.tuijian-list image {
	margin: 30rpx 30rpx;
	width: 180rpx;
	height: 180rpx;
	border-radius: 50%;
}
.tuijian-name {
	color: #3c3c3c;
	margin-top: 50rpx;
	margin-left: 20rpx;
	font-size: 33rpx;
	font-weight: bold;
}
.tuijian-detail {
	color: #919191;
	margin-top: 10rpx;
	margin-left: 20rpx;
	font-size: 28rpx;
}
.tuijian-skill {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.tuijian-skill-main {
	margin-top: 20rpx;
	margin-left: 20rpx;
	padding: 5rpx 10rpx;
	font-size: 25rpx;
	border: 2rpx solid #e7e6ec;
	border-radius: 10rpx;
	color: #c1c1c1;
}
.tuijan-label {
	position: absolute;
	top: 50rpx;
	right: 30rpx;
	font-size: 23rpx;
	color: #fff;
	background: #ff3366;
	padding: 2rpx 10rpx;
	border-radius: 5rpx;
}

