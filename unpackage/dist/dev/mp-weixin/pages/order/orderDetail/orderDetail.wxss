
@font-face {
	font-family: "iconfont";
	src: url('https://at.alicdn.com/t/iconfont.eot?t=1545902147596');
	src: url('https://at.alicdn.com/t/iconfont.eot?t=1545902147596#iefix') format('embedded-opentype'),
		url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
		url('https://at.alicdn.com/t/iconfont.ttf?t=1545902147596') format('truetype'),
		url('https://at.alicdn.com/t/iconfont.svg?t=1545902147596#iconfont') format('svg');
}
.iconfont {
	font-family: "iconfont" !important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.icon-dizhi:before {
	content: "\E652";
}
page {
	min-height: 100%;
	background-color: #f5f5f5;
}
#orderDetail {
	width: 100%;
	height: 100%;
}
.orderStatus {
	background: -webkit-gradient(linear ,left top, left bottom ,from(#3598DC), to(#63B8FF));
	background: -o-linear-gradient(top ,#3598DC, #63B8FF);
	background: linear-gradient(to bottom ,#3598DC, #63B8FF);
	background-size: 100% 100%;
	width: 100%;
	height: 200rpx;
	position: relative;
}
.orderStatusText {
	margin-left: 5%;
	color: #fff;
	height: 50rpx;
	font-size: 30rpx;
	line-height: 200rpx;
}
.orderGoods {
	margin-top: 20rpx;
	background: #fff;
}
.orderGoods-title {
	margin-left: 5%;
	font-size: 28rpx;
	font-weight: bold;
	color: #585858;
	line-height: 80rpx;
	border-bottom: 1px solid #f5f5f;
}
.orderAddress {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding-bottom: 10rpx;
	padding-top: 10rpx;
	background: #fff;
}
.icon-dizhi {
	color: #3598dc;
	line-height: 100rpx;
	font-size: 50rpx;
	margin-left: 5%;
	margin-right: 20rpx;
}
.orderAddress-main {
	width: 80%;
}
.orderAddress-main-name {
	font-size: 28rpx;
	margin-right: 20rpx;
}
.orderAddress-main-phone {
	font-size: 25rpx;
	color: #a8a8a8;
}
.orderAddress-main-pos {
	font-size: 28rpx;
}
.goods-list {
	width: 100%;
	margin-top: 20rpx;
	background-color: #fff;
	border-bottom: 10rpx solid #f5f5f5;
}
.goods-list .list-title {
	font-size: 28rpx;
	color: #585858;
	font-weight: bold;
	margin-left: 5%;
	line-height: 80rpx;
}
.goods-list .a-goods {
	width: 90%;
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	/*justify-content: space-between;*/
	border-top: 1px solid #eee;
}
.goods-list .a-goods .img-box {
	width: 160rpx;
	height: 160rpx;
	overflow: hidden;
	margin-top: 20rpx;
	margin-right: 20rpx;
	background-color: #d8d8d8;
}
.goods-list .img-box .img {
	width: 160rpx;
	height: 160rpx;
}
.goods-list .a-goods .text-box {
	width: 510rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding-top: 10rpx;
}
.a-goods .text-box .arow {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.a-goods .text-box .arow .goods-name {
	width: 360rpx;
	font-size: 26rpx;
	height: 74rpx;
	color: #000;
	line-height: 1.6;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
.a-goods .text-box .arow .goods-price {
	font-size: 26rpx;
	color: #000;
	-webkit-align-self: flex-start;
	    -ms-flex-item-align: start;
	        align-self: flex-start;
}
.a-goods .text-box .arow .goods-label {
	font-size: 26rpx;
	margin-top: 30rpx;
	width: 70%;
	color: #999;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
}
.a-goods .text-box .arow .goods-num {
	font-size: 26rpx;
	color: #999;
}
.orderPrice {
	width: 100%;
	font-size: 28rpx;
	position: relative;
	line-height: 80rpx;
}
.orderPrice-text {
	margin-left: 5%;
}
.orderPrice-num {
	position: absolute;
	top: 0rpx;
	right: 5%;
}
.orderPayPrice {
	width: 100%;
	font-size: 28rpx;
	position: relative;
	line-height: 80rpx;
}
.orderPayPrice-text {
	border-top: 1rpx solid #f5f5f5;
	margin-left: 5%;
}
.orderPayPrice-num {
	position: absolute;
	top: 0rpx;
	right: 5%;
	color: #ef696c;
}
.orderMes {
	background: #fff;
	margin-top: 20rpx;
}
.orderMes-title {
	font-size: 28rpx;
	color: #585858;
	font-weight: bold;
	margin-left: 5%;
	line-height: 80rpx;
	border-bottom: 1px solid #f5f5f5;
}
.orderNum {
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 28rpx;
	border-bottom: 1px solid #f5f5f5;
	height: 70rpx;
	line-height: 70rpx;
	position: relative;
}
.orderNum-number {
	position: absolute;
	top: 0rpx;
	right: 5%;
}
.orderTime {
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 28rpx;
	height: 70rpx;
	line-height: 70rpx;
	border-bottom: 1px solid #f5f5f5;
	position: relative;
}
.orderTime-number {
	position: absolute;
	top: 0rpx;
	right: 5%;
}
.orderPayWay {
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 28rpx;
	height: 70rpx;
	line-height: 70rpx;
	border-bottom: 1px solid #f5f5f5;
	position: relative;
}
.orderPayWay-number {
	position: absolute;
	top: 0rpx;
	right: 5%;
}
.orderWay {
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 28rpx;
	height: 70rpx;
	line-height: 70rpx;
	border-bottom: 1px solid #f5f5f5;
	position: relative;
	margin-bottom: 120rpx;
}
.orderPayWay-number {
	position: absolute;
	top: 0rpx;
	right: 5%;
}
.barBottom {
	height: 90rpx;
	position: fixed;
	bottom: 0;
	width: 100%;
	background: #fff;
	border-top: 1rpx solid #ddd;
}
.orderStatusBtn {
	position: absolute;
	right: 5%;
	top: 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 25rpx;
	height: 70rpx;
}
.orderStatusBtn button{
	margin-left: 10rpx;
}




