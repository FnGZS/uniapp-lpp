<view><scroll-view class="bg-white response cu-steps steps-bottom" scroll-x scroll-into-view="{{'scroll-'+scroll}}" scroll-with-animation><block wx:for="{{basicsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item padding-lr-xl',index>scroll?'':'text-blue']}}" id="{{'scroll-'+index}}"><text class="num" data-index="{{index+1}}"></text>{{''+item.name+''}}</view></block></scroll-view><view class="subPeople"><image src="https://www.sxscott.com/img/c.png"></image><view class="subPeople-info"><view class="subPeople-name">{{detail.name}}</view><view class="subPeople-detail">{{detail.place+" | "+detail.work+'年'}}</view><view class="subPeople-skill"><view class="subPeople-skill-main">{{detail.card}}</view></view><view class="subPeople-label"><view class="iconfont icon-dianhua" data-phone="{{detail.telephone}}" data-event-opts="{{[['tap',[['phone',['$event']]]]]}}" bindtap="__e">咨询</view></view></view></view><view class="subPeopleTime"><view class="public-title"><view class="public-leftLine"></view><view class="public-box color_5a6c81 font_29">排班</view></view><view class="subPeopleTimeTable _table"><view class="_tbody"><view class="_tr"><view class="_td"></view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="_td"><label class="_span">{{item.workTime}}</label><label class="_span">{{item.workDate}}</label></view></block></view><view class="_tr"><view class="font_28 _td">上午</view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="_td"><block wx:if="{{yuyueList[item.workDate][0]==0}}"><view class="{{['subPeopleTimeTableYuYue ',chooseType=='mor'&&chooseData==item.workDate?'choose':'']}}" data-type="mor" data-data="{{item.workDate}}" data-event-opts="{{[['tap',[['yuyue',['$event']]]]]}}" bindtap="__e">预约</view></block><block wx:if="{{yuyueList[item.workDate][0]==1}}"><view class="{{['subPeopleTimeTableNoYuYue',chooseType=='mor'&&chooseData==item.workDate?'choose':'']}}" data-type="'mor'">预约</view></block></view></block></view><view class="_tr"><view class="font_28 _td">下午</view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="_td"><block wx:if="{{yuyueList[item.workDate][1]==0}}"><view class="{{['subPeopleTimeTableYuYue',chooseType=='aft'&&chooseData==item.workDate?'choose':'']}}" data-type="aft" data-data="{{item.workDate}}" data-event-opts="{{[['tap',[['yuyue',['$event']]]]]}}" bindtap="__e">预约</view></block><block wx:if="{{yuyueList[item.workDate][1]==1}}"><view class="subPeopleTimeTableNoYuYue" data-type="aft">预约</view></block></view></block></view></view></view></view><view class="subPeopleInfo"><view class="public-title"><view class="public-leftLine"></view><view class="public-box color_5a6c81 font_29">简介</view></view><view class="subPeopleInfoText">{{detail.introduction}}</view></view><view data-event-opts="{{[['tap',[['toSub',['$event']]]]]}}" class="sub" bindtap="__e">下一步</view></view>