<view><view class="{{['header',(isH5Plus)?'status':'']}}"><view class="userInfo"><view class="face"><image src="{{userInfo.headimgurl}}"></image></view><view class="info"><view class="username">{{userInfo.nickName}}</view><view class="integral">普通用户</view></view></view><view class="setting"></view></view><view class="orders"><view class="box"><block wx:for="{{orderTypeLise}}" wx:for-item="row" wx:for-index="index" wx:key="name"><view class="label" hover-class="hover" data-event-opts="{{[['tap',[['toOrderType',[index]]]]]}}" bindtap="__e"><view class="icon"><block wx:if="{{row.badge>0}}"><view class="badge">{{row.badge}}</view></block><view class="{{['iconfont ',row.icon]}}"></view></view>{{''+row.name+''}}</view></block></view></view><block wx:for="{{severList}}" wx:for-item="list" wx:for-index="list_i" wx:key="list_i"><view class="list"><block wx:for="{{list}}" wx:for-item="li" wx:for-index="li_i" wx:key="name"><block wx:if="{{userDetail.role==li.role||li.role==''}}"><view class="{{['li',(li_i==list.length-1)?'noborder':'']}}" hover-class="hover" data-event-opts="{{[['tap',[['toPage',[list_i,li_i]]]]]}}" bindtap="__e"><view class="icon"><view class="{{['iconfont',li.icon]}}"></view></view><view class="text">{{li.name}}</view><view class="iconfont icon-jiantou"></view><block wx:if="{{li.icon=='icon-kefu'}}"><button class="service-kefu-btn" plain open-type="contact"></button></block></view></block></block></view></block></view>